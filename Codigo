    operacion="+";
    Escribir.setText("");
    }                                        

    private void jButton16ActionPerformed(java.awt.event.ActionEvent evt) {                                          
     n2=Float.parseFloat(Escribir.getText());
     switch(operacion){
     case "+":
         Escribir.setText(Float.toString(n1+n2));
         break;
         
     case "-":
         Escribir.setText(Float.toString(n1-n2));
         break;
     case "/":
         if(n2==0){
              Escribir.setText("No existe división por cero. ERROR");
              break;
         }
         Escribir.setText(Float.toString(n1/n2));
         break;
    case "*":
         Escribir.setText(Float.toString(n1*n2));
         break;
    case "√":
         if(n2<0 && n1%2==0){
              Escribir.setText("Indefinido calcular raiz del negativo");
              break;
         }
         else {
              Escribir.setText(Float.toString((float) Math.pow(n2, 1/n1)));
         break;
         }
        
    case "^":
         Escribir.setText(Float.toString((float) Math.pow(n1, n2)));
         break;
    case "%":
         Escribir.setText(Float.toString(n1*n2/100));
         break;
    case "sin":
         Escribir.setText(Float.toString((float)Math.sin(Math.toRadians(n2))));
         break;
    case "cos":
         Escribir.setText(Float.toString((float)Math.cos(Math.toRadians(n2))));
         break;
    /*case "tan":
         if(n2==90){
              Escribir.setText("El resultado es indeterminado");
              break;
         }
         if(n2==270){
              Escribir.setText("El resultado es indeterminado");
              break;
         }
         Escribir.setText(Float.toString((float)Math.tan(Math.toRadians(n2))));
         break;*/
         }  
    }                                         

    private void jButton13ActionPerformed(java.awt.event.ActionEvent evt) {                                          
    n1=Float.parseFloat(Escribir.getText());
    operacion="√";
    Escribir.setText("");
    }                                         

    private void jButton19ActionPerformed(java.awt.event.ActionEvent evt) {                                          
    n1=Float.parseFloat(Escribir.getText());
    operacion="^";
    Escribir.setText("");
    }                                         

    private void jButton20ActionPerformed(java.awt.event.ActionEvent evt) {                                          
